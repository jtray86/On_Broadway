<h1>Edit Profile</h1>
    <%= form_for @actor do |f|%>

        <%=f.label :name%>
        <%=f.text_field :name%>
        <br>
        <%=f.label :bio%>
        <%=f.text_field :bio%>
        <br>
        <%=f.label :website%>
        <%=f.text_field :website%>
        <br>
        <%=f.label :image%>
        <%=f.text_field :image%>
        <br>
        <%=f.submit "Edit Profile"%>
    <%end%>

    <%if flash[:message]%>
        <%flash[:message].each do |message|%>
            <p><%=message%></p>
        <%end%>
    <%end%>