<h1> Add a Creative to the Team </h1>

<%=form_for @creative_show do |f|%>
        <%=f.label :creative_id %> 
        <%=f.collection_select :creative_id, Creative.all, :id, :name %> 
        <%=f.label :show_id %> 
        <%=f.collection_select :show_id, Show.all, :id, :name %> 
        <%=f.submit "Add them to the Creative Team" %> 
    <%end%> 