<h1>Login</h1>
<%= form_tag "/please_login", method: "post" do%>
    <%= label_tag :username%>
    <%= text_field_tag :username%>
    <%= label_tag :password%>
    <%= password_field_tag :password%>
    <%=submit_tag "Login"%>
<%end%>

    <%= button_to "Signup", new_admin_path, method: "get" %>

<%if flash[:message]%>
    <p><%=flash[:message]%></p> 
<%end%>