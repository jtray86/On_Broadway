<h1>Cast Actor</h1>


  

    <%=form_for @actor_character do |f| %>
        <%=f.label :actor_id %>
        <%=f.text_field :actor_id %>
        <%=f.label :character_id %>
        <%=f.text_field :character_id %>
        <%=f.submit "Add Actor and Character" %>

    <%end%>
         
    <%if flash[:message]%>
            <p><%=flash[:message]%></p>
    <%end%>

